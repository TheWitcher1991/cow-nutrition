{
    "trailingComma": "all",
    "tabWidth": 4,
    "useTabs": true,
    "semi": false,
    "jsxSingleQuote": true,
    "singleQuote": true,
    "arrowParens": "avoid",
    "importOrder": [
      "<THIRD_PARTY_MODULES>",
      "^./(.*)",
      "(.css)$"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderCaseInsensitive": true
  }